# Install editor-config package from Atom
# and customise project layout here

# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = crlf
insert_final_newline = true
indent_style = space
indent_size = 4
charset = utf-8

[*.html]
indent_size = 2
